<Defs>  

  <ThingDef ParentName="BuildingBase">
    <defName>VME_AncientSystemRack</defName>
    <label>ancient system rack</label>
    <description>An ancient rack unit packed with unidentifiable devices. Valuable parts are missing, and everything else is degraded to uselessness.</description>
    <graphicData>
      <texPath>Things/Building/Ruins/AncientSystemRack</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(1, 3)</drawSize>
      <shadowData>
        <volume>(0.9, 0.4, 3)</volume>
      </shadowData>
    </graphicData>
	<tradeability>None</tradeability>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsMisc</li>
		</thingCategories>
    <size>(1, 3)</size>
    <fillPercent>0.40</fillPercent>
    <altitudeLayer>Building</altitudeLayer>
    <statBases>
      <MaxHitPoints>250</MaxHitPoints>
      <WorkToBuild>1500</WorkToBuild>
      <Flammability>0.5</Flammability>
	  <Mass>25</Mass>
    </statBases>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
  </ThingDef>

