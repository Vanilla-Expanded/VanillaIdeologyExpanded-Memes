  <ThingDef ParentName="BuildingBase">
    <defName>VME_AncientLamp</defName>
    <label>ancient lamp</label>
    <description>An ancient self-powered lamp. Its internal power supply is badly depleted, but it still emits a weak glow.</description>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <selectable>true</selectable>
    <fillPercent>0.20</fillPercent>
    <pathCost>14</pathCost>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
    <rotatable>false</rotatable>
    <graphicData>
      <graphicClass>Graphic_Single</graphicClass>
      <texPath>Things/Building/Ruins/AncientLamp</texPath>
    </graphicData>
	<tradeability>None</tradeability>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsMisc</li>
		</thingCategories>
    <comps>
      <li Class="CompProperties_Glower">
        <glowRadius>5.5</glowRadius>
        <glowColor>(71,43,6,0)</glowColor>
      </li>
    </comps>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <Flammability>0.5</Flammability>
	  <Mass>10</Mass>
    </statBases>
  </ThingDef>

  <ThingDef ParentName="BuildingBase">
    <defName>VME_AncientBed</defName>
    <label>ancient bed</label>
    <thingClass>Building_Bed</thingClass>
    <description>An old, rotted mattress on a rusty frame. Though uncomfortable, it can still be used, but it's too dilapidated to restore.</description>
    <graphicData>
      <texPath>Things/Building/Ruins/AncientBed</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(2, 2)</drawSize>
      <damageData>
        <rect>(0,0.05,1,0.95)</rect>
      </damageData>
    </graphicData>
	<tradeability>None</tradeability>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsMisc</li>
		</thingCategories>
    <statBases>
      <MaxHitPoints>140</MaxHitPoints>
      <Flammability>0.8</Flammability>
      <Beauty>1</Beauty>
      <WorkToBuild>800</WorkToBuild>
      <Mass>20</Mass>
      <BedRestEffectiveness>0.85</BedRestEffectiveness>
      <Comfort>0.75</Comfort>
      <ImmunityGainSpeedFactor>0.95</ImmunityGainSpeedFactor>
      <SurgerySuccessChanceFactor>0.65</SurgerySuccessChanceFactor>
    </statBases>
    <size>(1,2)</size>
    <building>
      <bed_showSleeperBody>True</bed_showSleeperBody>
      <bed_healPerDay>1</bed_healPerDay>
    </building>
    <comps>
      <li Class="CompProperties_AssignableToPawn">
        <drawAssignmentOverlay>false</drawAssignmentOverlay>
        <compClass>CompAssignableToPawn_Bed</compClass>
      </li>
    </comps>
    <fillPercent>0.4</fillPercent>
    <pathCost>42</pathCost>
    <staticSunShadowHeight>0.17</staticSunShadowHeight>
    <castEdgeShadows>True</castEdgeShadows>
    <techLevel>Medieval</techLevel>
  </ThingDef>

