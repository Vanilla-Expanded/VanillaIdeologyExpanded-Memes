<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<RulePackDef>
		<defName>VME_NamerDeityEldritch</defName>
		<include>
			<li>GlobalUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>r_deityName->[start][middle][end]</li>
				<li>start->cn</li>
				<li>start->kt</li>
				<li>start->c't</li>
				<li>start->kh</li>
				<li>start->yir</li>
				<li>start->yor</li>
				<li>start->yok</li>
				<li>start->igh</li>
				<li>start->vhal</li>
				<li>start->gher</li>
				<li>start->mh'ur</li>
				<li>start->vh'or</li>
				<li>start->og</li>
				<li>start->ak</li>
				<li>start->kol</li>
				<li>start->okt</li>
				<li>start->yom-</li>
				<li>start->sub-</li>
				<li>start->ed</li>
				<li>start->ith</li>
				<!-- Middle is always surrounded by vowels -->
				<li>middle->all'ra</li>
				<li>middle->hele</li>
				<li>middle->ujhu</li>
				<li>middle->aand'xu</li>
				<li>middle->'og'vhe</li>
				<li>middle->aathe</li>
				<li>middle->omliggda</li>
				<li>middle->ethla</li>
				<li>middle->ouvha</li>
				<li>middle->omlao</li>
				<!-- Ends never begin with vowels -->
				<li>end(p=4)-></li>
				<li>end->tl</li>
				<li>end->ss</li>
				<li>end->xr</li>
				<li>end->rc</li>
				<li>end->sh</li>
				<li>end->l</li>
				<li>end->va</li>
				<li>end->rrath</li>
				<li>end->th</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>
